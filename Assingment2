//Task 1
class Task1{
    public static void main(String[] args){
int hungry; //valid, Legal Identifier
int 2AB; //invalid, variable name can not start with a number
int 312.2; //invalid, variable name can not start with a number
int MOBILE; //valid, Legal Identifier
int "Ans"; //invalid, variable name can not start with a double quotation
int $30; //valid, Legal identifier
int Yes/No; //valid, Legal identifier
int student-id; //valid, Legal identifier
int A+3; //valid, Legal identifier
int 'X'; //invalid, variable name can not start with a double quotation
int return; //valid, Legal identifier
    }
}






//Task2 
class Task2{
    public static void main(String[] args){
        //2.1
        int num1 = 8;
        System.out.pritnln(num1);

        //2.2
        int num2 = 4;
        System.out.pritnln(num1+num2);
         
        //2.3
        System.out.pritnln(num1*num2);
        System.out.pritnln(num1/num2);

        //2.4
        double num3 = 8.5d;
        System.out.println(num3);

        double num4 = 4.5d;
        System.out.println(num3+num4);
        System.out.println(num3*num4);
        System.out.println(num3/num4);

        //2.5
        double num5 = 6.5d;
        System.out.println(num5);
        int num6 = 2;
        System.out.println(num5+num6);
        System.out.println(num5*num6);
        System.out.println(num5/num6);

        //2.6
String num7 = "Hello";
System.out.println(num7);
String num8 = "ST";
System.out.println(num7+num8);  //addition operator connects the variable for data type string,  result: HelloST

int num9 = 7;
String num10 = "ST";
System.out.println(num9+num10);   //the variable will connect with each other , result: 7ST

String num10 = "Hello";
int num9 = 8;
System.out.println(num9+num10);   //the variable will connect with each other , result: Hello8

}
}




//Task3
class Task3{
    public static void public static void main(String[] args) {
        double cr,ar;
        int a = 4;
        cr = 2*3.1416*a;
        ar = 3.1416*a*a;
        System.out.println("Your Circumference is"+cr);
        System.out.println("Your Area is "+ar);
}
}





