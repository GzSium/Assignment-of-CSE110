//Task6
class task6{
    public static void main(String[] args){

        //a
        int a = 3;
        int b = 4;
        int c;
        c=b;
        b=a;
        a=c;
        System.out.println(a);
        System.out.println(b);

        //b
        int a = 3;
        int b = 4;
        a= a+b;
        b= a-b;
        a= a-b;
        System.out.println(a);
        System.out.println(b);
    }
}



//Task 7 
class task7{
    public static void main(String[] args){
long min = 98765345;
long hourandday =60*24;
long mininyear = hourandday*365;
long totalyear = min / mininyear ;
long remainMin = min % mininyear ;
long remainDay = remainMin/ hourandday;
System.out.println(min+" minutes is approximately " +totalyear+" years and"+remainDay+ "days");
    }
}




//Task 8
class Task8{
    public static void main(String [] args){
        int a,b,c,d;
        a = 2;
        b = 5;
        c = 8;
d = (2*b*(c-a)/3)+7;
System.out.println("The answer is "+d);
    }
}




//Task 9
class Task9{
    public static void main(String[] args){
        int n = 7;
        int n1,n2,n3,n4,n5,n6,n7,n8,n9,n10;
        n1 = n*1;
        n2 = n*2;
        n3 = n*3;
        n4 = n*4;
        n5 = n*5;
        n6 = n*6;
        n7 = n*7;
        n8 = n*8;
        n9 = n*9;
        n10 = n*10;
        System.out.println("7 x 1="+n1);
        System.out.println("7 x 2="+n2);
        System.out.println("7 x 3="+n3);
        System.out.println("7 x 4="+n4);
        System.out.println("7 x 5="+n5);
        System.out.println("7 x 6="+n6);
        System.out.println("7 x 7="+n7);
        System.out.println("7 x 8="+n8);
        System.out.println("7 x 9="+n8);
        System.out.println("7 x 10="+n10);
    }
}



//Task 10
class task10{
    public static void main(String[] args){
        int n,a,L,S;
         n = 100;
         a = 1;
         L  = 100;
    S = n/2*(a+L);
    System.out.println("The sum of the first 100 positive number is "+S);

    }
}




//Task 11
class task11{
    public static void main(String[] args){
        double a = 4.5d;
        double b = 9.5d;
        double c = Math.sqrt(a*a + b*b);
        double sinA,sinB,cosA,cosB;
        sinA = a/c;
        sinB = b/c;
        cosA = b/c;
        cosB = a/c;
        System.out.println("SinA ="+sinA);
        System.out.println("SinB="+sinB);
        System.out.println("CosA ="+cosA);
        System.out.println("CosB ="+cosB);
    }
}



