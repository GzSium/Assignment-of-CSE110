        x = n%100;
        l2 = x/10;
        System.out.println(l2);

    }
}



//Task13
public class Task13{
    public static void main (String[] args){
      
      
      double hour = 5;
      double min = 56;
      double sec = 23;
      double n = hour+(min/60)+(sec/3600);
      double d = 1267;
      double dinkm = 1264/1000;
      double dinmile = 1264/1609;
      double vinkm = dinkm/n;
      double vinmile = dinmile/n;
      System.out.println("Your velocity in km/h is " +vinkm);
        System.out.println("Your velocity in mile/h is " +vinmile);
    }
  }





//TASK14
class Task14 {
  
  public static void main (String[] args){
    
    int a = 8;
    int b = 3;
    int h = a/2;
    double hex_side1 = Math.sqrt(h*h+b*b);
    double hex_area = ((3*Math.sqrt(3))/2)*(hex_side1*hex_side1);
    double hex_circumference = 6*hex_side1;
    System.out.println("Area :"+hex_area);
     System.out.println("Circumference :"+hex_circumference);
}
}
      
